{
  "name": "spring-boot-admin-server-ui",
  "private": true,
  "description": "Spring Boot Admin UI",
  "scripts": {
    "build": "vite build --emptyOutDir",
    "build:dev": "NODE_ENV=development vite build --emptyOutDir --sourcemap --mode development",
    "build:watch": "NODE_ENV=development vite build --emptyOutDir --watch --sourcemap --mode development",
    "dev": "vite",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --ext .js,.vue src/main/frontend",
    "lint:fix": "eslint --ext .js,.vue --fix src/main/frontend",
    "format": "prettier src/main/frontend",
    "format:fix": "prettier src/main/frontend --write"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/vue-fontawesome": "3.0.3",
    "@headlessui/vue": "1.7.9",
    "@stekoe/vue-toast-notificationcenter": "https://github.com/SteKoe/vue-toast-notificationcenter/archive/refs/tags/1.0.0-RC3.tar.gz",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "ansi_up": "5.1.0",
    "autolinker": "3.16.2",
    "axios": "0.27.2",
    "chart.js": "4.2.0",
    "chartjs-adapter-moment": "1.0.1",
    "classnames": "2.3.2",
    "d3-array": "3.2.2",
    "d3-axis": "3.0.0",
    "d3-brush": "3.0.0",
    "d3-scale": "4.0.2",
    "d3-selection": "3.0.0",
    "d3-shape": "3.2.0",
    "d3-time": "3.1.0",
    "event-source-polyfill": "1.0.31",
    "file-saver": "2.0.5",
    "iso8601-duration": "1.3.0",
    "lodash-es": "4.17.21",
    "mitt": "^3.0.0",
    "moment": "2.29.4",
    "popper.js": "1.16.1",
    "pretty-bytes": "6.1.0",
    "resize-observer-polyfill": "1.5.1",
    "rxjs": "7.8.0",
    "v3-infinite-loading": "1.2.2",
    "vue": "3.2.47",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6",
    "vue3-click-away": "1.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/eslint-parser": "7.19.1",
    "@babel/plugin-transform-runtime": "7.19.6",
    "@babel/preset-env": "7.20.2",
    "@babel/runtime": "7.20.13",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-vite": "0.4.0",
    "@storybook/vue3": "6.5.16",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/user-event": "14.4.3",
    "@testing-library/vue": "6.6.1",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/test-utils": "2.2.10",
    "@vue/vue3-jest": "29.2.2",
    "autoprefixer": "10.4.13",
    "babel-jest": "29.4.1",
    "babel-loader": "9.1.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "0.6.10",
    "eslint-plugin-vue": "^9.0.0",
    "jest": "29.4.1",
    "jest-environment-jsdom": "29.4.1",
    "jest-transform-stub": "2.0.0",
    "msw": "1.0.0",
    "msw-storybook-addon": "1.7.0",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "^1.57.1",
    "storybook-vue3-router": "2.3.1",
    "tailwindcss": "3.2.4",
    "vite": "4.1.1",
    "vite-plugin-static-copy": "0.13.0",
    "vue-loader": "17.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/.jest/jest.setup.js"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "testEnvironmentOptions": {
      "url": "https://example.com",
      "customExportConditions": [
        "node",
        "node-addons"
      ]
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(d3-.*|internmap|lodash-es|pretty-bytes)/)"
    ],
    "transform": {
      ".+\\.vue$": "@vue/vue3-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      ".+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/main/frontend/$1"
    },
    "testMatch": [
      "**/*.spec.js"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "msw": {
    "workerDirectory": "./public"
  }
}
